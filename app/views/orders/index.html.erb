<p id="notice"><%= notice %></p>

<h1>Orders</h1>

<table>
  <thead>
    <tr>
      <th>Customer</th>
      <th>Model</th>
      <th>Serial</th>
      <th>Brand</th>
      <th>Callsid</th>
      <th>Symptom</th>
      <th>Status</th>
      <th>Agent</th>
      <th>Other</th>
      <th>Address</th>
      <th>Window</th>
      <th>Technician</th>
      <th>Day</th>
      <th>Other</th>
      <th>Invoicenumber</th>
      <th>Invoicetype</th>
      <th>Laborcode</th>
      <th>Part</th>
      <th>Amtdue</th>
      <th>Taxamt</th>
      <th>Other</th>
      <th colspan="3"></th>
    </tr>
  </thead>

  <tbody>
    <% @orders.each do |order| %>
      <tr>
        <td><%= order.customer.name %></td>
        <td><%= order.model %></td>
        <td><%= order.serial %></td>
        <td><%= order.brand %></td>
        <td><%= order.callsid %></td>
        <td><%= order.symptom %></td>
        <td><%= order.status %></td>
        <td><%= order.agent_id %></td>
        <td><%= order.other %></td>
        <td><%= order.address %></td>
        <td><%= order.window %></td>
        <td><%= order.technician_id %></td>
        <td><%= order.day %></td>
        <td><%= order.other %></td>
        <td><%= order.invoiceNumber %></td>
        <td><%= order.invoicetype %></td>
        <td><%= order.laborCode %></td>
        <td><%= order.part %></td>
        <td><%= order.amtDue %></td>
        <td><%= order.taxAmt %></td>
        <td><%= order.other %></td>
        <td><%= link_to 'Show Customer', [@customer] %></td>
        <td><%= link_to 'Open Order data', edit_customer_order_path(@customer, order) %></td>
        <td><%= link_to 'Destroy Order', order, method: :delete, data: { confirm: 'Are you sure?' } %></td>
      </tr>
    <% end %>
  </tbody>
</table>

<br>

<%= link_to 'New Order', new_customer_order_path(@customer) %>
